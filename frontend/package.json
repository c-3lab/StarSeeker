{
  "name": "sample-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write .",
    "fix": "npm run lint && npm run format",
    "check-types": "tsc",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@react-leaflet/core": "^1.1.0",
    "@types/node": "^16.6.0",
    "@types/react": "^17.0.17",
    "express": "^4.17.1",
    "leaflet": "^1.7.1",
    "next": "11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-leaflet": "^3.2.1",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/leaflet": "^1.7.4",
    "@types/react-leaflet": "^2.8.2",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2"
  }
}
